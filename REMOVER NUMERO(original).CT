  <LuaScript>-- Nome do processo que o trainer deve procurar
local processName = "mameui32.exe"

-- Função que tenta anexar ao processo e ativar o cheat
local function autoAttach()
  if getProcessIDFromProcessName(processName) ~= nil then
    openProcess(processName)

    -- Substitua '0' pelo índice do seu script na lista (geralmente é 0 se for o primeiro)
    -- Isso ativa o script de salto e limpeza automaticamente
    getAddressList().getMemoryRecord(0).Active = true
    -- Se quiser que o trainer feche após ativar, descomente a linha abaixo:
    -- closeCE()
  end
end

-- Cria um timer que verifica a cada 1 segundo (1000ms) se o MAME abriu
local t = createTimer(nil)
t.Interval = 1000
t.OnTimer = autoAttach

</LuaScript>
