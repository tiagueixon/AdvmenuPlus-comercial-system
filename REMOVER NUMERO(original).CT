    <CheatEntry>
      <ID>0</ID>
      <Description>"Remover Numero"</Description>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[ENABLE]
// 1. O SALTO: Impede que o MAME reescreva o número a cada frame
00A3C7BD:
jmp 00A3C7F8
nop
nop
nop
nop

// 2. A LIMPEZA: Apaga o que já está escrito no endereço da string
// Preenche 30 bytes com o valor 0 (null) no endereço inicial da string
027D4BA0:
db 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00 00

[DISABLE]
// Restaura a instrução original de escrita
00A3C7BD:
db C7 05 A0 4B 7D 02 4D 75 6C 00

// Nota: A string não precisa ser restaurada manualmente aqui,
// pois o próprio MAME voltará a escrevê-la assim que o 'jmp' for removido.
</AssemblerScript>
    </CheatEntry>
